{% for resume in resume_list %}
    <div class="col-xl-3 col-sm-6 mb-3">
        <div class="card text-white bg-primary o-hidden h-100">
            <div class="card-body">
                {% if resume.profilePicture is defined and resume.profilePicture != null %}
                    <div class="card-body-icon">
                        <img class="img-thumbnail"
                             src="{{ asset('bundles/resumeprofiler/assets/images/samir.png') }}"
                             alt="Pict"/>
                    </div>
                {% else %}
                    <div class="card-body-icon">
                        <i class="fas fa-fw fa-user"></i>
                    </div>
                {% endif %}
                <div class="mr-5">{{ resume.name }}</div>
            </div>
            <a class="card-footer text-white clearfix small z-1" href="{{ path('resume_detail',{resume_name:"dsd"}) }}">
                <span class="float-left">View Details</span>
                <span class="float-right">
                    <i class="fas fa-angle-right"></i>
                  </span>
            </a>
        </div>
    </div>
{% endfor %}